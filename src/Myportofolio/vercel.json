{
  "version": 2,
  "builds": [
    {
      "src": "votre_projet/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "votre_projet/wsgi.py"
    }
  ]
}